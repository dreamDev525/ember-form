<input
  type="checkbox"
  {{action "change" on="change"}}
  {{action "input" on="input"}}
  
  checked={{checked}}
  name={{mainComponent.name}}
  id={{mainComponent.id}}
  disabled={{mainComponent.disabled}}
  class={{mainComponent.elementClass}}
  required={{mainComponent.required}}
  autofocus={{mainComponent.autofocus}}
/>

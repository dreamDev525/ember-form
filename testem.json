{
    "framework": "qunit",
    "before_tests": "coffee -c tests/*.spec.coffee",
    "after_tests": "rm tests/*.spec.js",
    "src_files": [
      "tests/**/*.spec.coffee"
    ],
    "serve_files": [
      "vendor/jquery/dist/jquery.js",
      "vendor/handlebars/handlebars.js",
      "vendor/ember/ember.js",
      "vendor/ember-qunit/dist/globals/main.js",
      "dist/ember_forms.js",
      "test/support/setup.js",
      "test/**/*.spec.js"
    ],
    "launch_in_dev": ["PhantomJS"],
    "launch_in_ci": ["PhantomJS"]
}
